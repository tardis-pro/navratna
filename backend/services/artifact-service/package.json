{
  "name": "@uaip/artifact-service",
  "version": "1.0.0",
  "description": "UAIP Artifact Generation and Management Service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "bun --hot src/index.ts",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@uaip/config": "workspace:*",
    "@uaip/middleware": "workspace:*",
    "@uaip/shared-services": "workspace:*",
    "@uaip/types": "workspace:*",
    "@uaip/utils": "workspace:*",
    "cors": "catalog:backend",
    "helmet": "catalog:backend",
    "qrcode": "catalog:backend",
    "typeorm": "catalog:backend",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/cors": "catalog:backend",
    "@types/jest": "catalog:backend",
    "@types/node": "catalog:backend",
    "@types/qrcode": "catalog:backend",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:backend",
    "nodemon": "catalog:backend",
    "ts-node": "catalog:backend",
    "tsc-alias": "catalog:backend",
    "tsx": "catalog:backend",
    "typescript": "catalog:"
  },
  "keywords": [
    "uaip",
    "artifact",
    "generation",
    "validation",
    "microservice"
  ],
  "author": "UAIP Team",
  "license": "MIT"
}
