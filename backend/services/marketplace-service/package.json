{
  "name": "@uaip/marketplace-service",
  "version": "1.0.0",
  "description": "UAIP Marketplace Service - AI Agent and Persona Marketplace",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "bun --hot src/index.ts",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@uaip/config": "workspace:*",
    "@uaip/llm-service": "workspace:*",
    "@uaip/middleware": "workspace:*",
    "@uaip/shared-services": "workspace:*",
    "@uaip/types": "workspace:*",
    "@uaip/utils": "workspace:*",
    "compression": "catalog:backend",
    "cors": "catalog:backend",
    "dotenv": "catalog:backend",
    "helmet": "catalog:backend",
    "morgan": "^1.10.0",
    "node-cron": "^4.1.0",
    "prom-client": "catalog:backend",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/compression": "catalog:backend",
    "@types/cors": "catalog:backend",
    "@types/morgan": "^1.9.10",
    "@types/node": "catalog:backend",
    "@types/node-cron": "^3.0.11",
    "concurrently": "catalog:dev",
    "nodemon": "catalog:backend",
    "tsc-alias": "catalog:backend",
    "tsx": "catalog:backend",
    "typescript": "catalog:",
    "vitest": "catalog:dev"
