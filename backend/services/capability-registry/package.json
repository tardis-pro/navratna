{
  "name": "@uaip/capability-registry",
  "version": "1.0.0",
  "description": "UAIP Capability Registry Service - Manages agent capabilities and their relationships",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "bun --hot src/index.ts",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@uaip/config": "workspace:*",
    "@uaip/middleware": "workspace:*",
    "@uaip/shared-services": "workspace:*",
    "@uaip/types": "workspace:*",
    "@uaip/utils": "workspace:*",
    "axios": "catalog:backend",
    "bcrypt": "catalog:backend",
    "compression": "catalog:backend",
    "cors": "catalog:backend",
    "dotenv": "catalog:backend",
    "helmet": "catalog:backend",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "catalog:backend",
    "pg": "catalog:backend",
    "prom-client": "catalog:backend",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/bcrypt": "catalog:backend",
    "@types/compression": "catalog:backend",
    "@types/cors": "catalog:backend",
    "@types/jest": "catalog:backend",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.12",
    "@types/node": "catalog:backend",
    "@types/pg": "catalog:backend",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "concurrently": "catalog:dev",
    "eslint": "catalog:",
    "jest": "catalog:backend",
    "nodemon": "catalog:backend",
    "ts-jest": "catalog:backend",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "catalog:backend",
    "tsx": "catalog:backend",
    "typescript": "catalog:"
  },
  "keywords": [
    "uaip",
    "capability",
    "registry",
    "microservice"
  ],
  "author": "UAIP Team",
  "license": "MIT"
}
